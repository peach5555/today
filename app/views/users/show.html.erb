<div class="contents row">
  <p><%= @nickname %>さんのメニュー一覧</p>
  <% @articles.each do |article| %>
    <div class="content_post">
      <div class="article">
        <img src="<%= article.image %>" class="content_image">
        <div class="article_main">
          <h3><%= article.title %></h3>
          <%= simple_format(article.text) %>
        </div>
      </div>
      
    </div>
    
  <% end %>
  <%= paginate(@articles) %>
  
</div>